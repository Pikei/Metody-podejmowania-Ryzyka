procedure HookeJeeves()
    xStart = [100]
    for i from 0 to length(xStart) - 1
        xStart[i] = 1
    end for
    x = copy(xStart)
    k = 1.5
    e = 0.02

    while k > e do:
        krokProbny()
        krokRoboczy()
        if f(x) < f(xStart)
            xStart = x
    end while
end procedure

procedure krokProbny()
    v = k
    for i from 0 to length(x) - 1
        x[i] += k
        if f(x) < f(xStart)
            k = v
            continue
        x[i] -= 2 * k
        if f(x) < f(xStart)
            k = v
            continue
        x[i] = xStart[i]
        if k > e
            k *= 0.2
            i -= 1
        else
            k = v
    end for
end procedure

procedure krokRoboczy()
    v = k
    tempArray = copy(x)
    for i from 0 to length(x) - 1
        x[i] = xStart[i] + v * (x[i] - xStart[i])
    end for
    if f(x) < f(tempArray)
        k *= 1.25
    else
        x = tempArray
        k *= 0.2

end procedure